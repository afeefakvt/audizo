        <div class="main-panel">
            <div class="content-wrapper">
                <div class="page-header">
                    <h3 class="page-title">
                        <span class="page-title-icon bg-gradient-primary text-white mr-2">
                            <i class="mdi mdi-ticket-percent menu-icon"></i>
                        </span> Coupons
                    </h3>
                    <nav aria-label="breadcrumb">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item"><a href="/admin/home">Dashboard</a></li>
                            <li class="breadcrumb-item active" aria-current="page">Coupons</li>
                        </ol>
                        <div class="button-group" style="margin-top: 10px;">
                            <button class="btn btn-gradient-danger" data-toggle="modal"
                                data-target="#addCouponModal">Add Coupon</button>
                        </div>
                    </nav>
                </div>

                <div class="row">
                    <div class="col-12 grid-margin">
                        <div class="card">
                            <div class="card-body">
                                <div class="table-responsive">
                                    <table class="table table-striped table-hover">
                                        <thead class="text-center">
                                            <tr>
                                                <th>Coupon Code</th>
                                                <th>Percentage</th>
                                                <th>Min Price</th>
                                                <th>Redeem Amount</th>
                                                <th>Expiry Date</th>
                                                <th>Actions</th>
                                            </tr>
                                        </thead>
                                        <tbody class="text-center">
                                            <% if (coupon.length> 0) { %>
                                                <% coupon.forEach(coupon=> { %>
                                                    <tr>
                                                        <td>
                                                            <%= coupon.couponCode %>
                                                        </td>
                                                        <td>
                                                            <%= coupon.percentage %>
                                                        </td>
                                                        <td>&#8377; <%= coupon.minPrice %>
                                                        </td>
                                                        <td>&#8377; <%= coupon.maxRedeemAmount %>
                                                        </td>
                                                        <td>
                                                            <%= coupon.expiryDate.toLocaleDateString() %>
                                                        </td>
                                                        <td>
                                                            <a href="/admin/coupons/editCoupon?id=<%= coupon._id %>"
                                                                class="btn btn-gradient-primary btn-sm">Edit</a>
                                                            <a href="#" class="btn btn-gradient-danger btn-sm"
                                                                onclick="confirmDelete('<%= coupon._id %>')">Delete</a>
                                                        </td>
                                                    </tr>
                                                    <% }); %>
                                                        <% } %>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Add Coupon Modal -->
