    <div class="main-panel">
      <div class="content-wrapper">
        <div class="page-header">
          <h3 class="page-title">
            <span class="page-title-icon bg-gradient-primary text-white mr-2">
              <i class="mdi mdi-cart"></i>
            </span> Orders
          </h3>
          <nav aria-label="breadcrumb">
            <ol class="breadcrumb">
              <li class="breadcrumb-item"><a href="/admin/home">Dashboard</a></li>
              <li class="breadcrumb-item active" aria-current="page">orders</li>
            </ol>

          </nav>
        </div>


        <div class="col-lg-12 grid-margin stretch-card">
          <div class="card">
            <div class="card-body">
              <table class="table table-bordered table-responsive" style="width: 100%">
                <thead class="text-center">
                  <tr style="line-height: 1;">
                    <th>Order ID</th>
                    <th>Date</th>
                    <th>Name</th>
                    <th>Address</th>
                    <th>Order Status</th>
                    <th>Payment Method</th>
                    <th>Payment Status</th>
                    <th>Total Price</th>
                    <th>Product Name</th>
                    <th>Quantity</th>
                    <th>Price</th>
                    <th>Status</th>
                    <th>Manage</th>

                  </tr>
                </thead>
                <tbody>
                  <% if(orders.length> 0) {
                     for(let i = 0; i < orders.length; i++) { %>
                      <% for(let j=0; j < orders[i].items.length; j++) { %>
                        <tr class="text-center" style="line-height: 1;">
                          <% if(j===0) { %>
                            <td rowspan="<%= orders[i].items.length %>">
                              <%= orders[i].orderId %>
                            </td>
                            <td rowspan="<%= orders[i].items.length %>">
                              <%= orders[i].date.toLocaleDateString() %>
                            </td>
                            <td rowspan="<%= orders[i].items.length %>">
                              <%= orders[i].user.name %>
                            </td>
                            <td rowspan="<%= orders[i].items.length %>">
                              <%= orders[i].address.name %><br>
                                <%= orders[i].address.house %>
                                  <%= orders[i].address.locality %>,<%= orders[i].address.city %><br>
                                      <%= orders[i].address.state %>,<%= orders[i].address.pincode%> <br>
                                          <%= orders[i].address.mobile %>
                            </td>
                            <td rowspan="<%= orders[i].items.length %>" id="status<%=i%>">
                              <%= orders[i].status %>
                            </td>
                            <td rowspan="<%= orders[i].items.length %>" id="paymentMethod<%=i%>">
                              <%= orders[i].paymentMethod %>
                            </td>
                            <td rowspan="<%= orders[i].items.length %>" id="paymentStatus<%=i%>">
                              <%= orders[i].paymentStatus %>
                            </td>
                            <td rowspan="<%= orders[i].items.length %>">
                              <%= orders[i].totalPrice %>
                            </td>
                            <% } %>
                              <td style="padding: 0;margin: 0;">
                                <%= orders[i].items[j].productName %>
                              </td>
                              <td style="padding: 0;margin: 0;">
                                <%= orders[i].items[j].quantity %>
                              </td>
                              <td style="padding: 0;margin: 0;">
                                <%= orders[i].items[j].finalPrice %>
                              </td>
                              <td id="orderStatus<%=i%><%=j%>" style="padding: 0;margin: 0;">
                                <%= orders[i].items[j].itemStatus %>
                              </td>
                              <td style="padding: 0;margin: 0;">

                                <select class="form-control text-dark" id="currentStatus<%=i%><%=j%>"
                                  onchange="changeStatus('<%=orders[i]._id%>', '<%=i%><%=j%>', '<%= orders[i].items[j]._id %>','<%=i%>')"
                                  style="width: 150px;" <% if (orders[i].items[j].itemStatus==="Cancelled" ){ %>
                                  disabled <% } %>>

                                    <option value="Ordered" disabled <% if (orders[i].items[j].itemStatus==="Ordered" ) { %>
                                      selected <% } %>>Ordered</option>
                                    <option value="Shipped"  <% if (orders[i].items[j].itemStatus==="Shipped" ) { %>
                                      selected <% } %>>Shipped</option>
                                    <option value="Delivered"  <% if (orders[i].items[j].itemStatus==="Delivered" ) {%>
                                      selected <% } %>>Delivered</option>
                                    <option value="Cancelled" disabled <% if(orders[i].items[j].itemStatus==="Cancelled"
                                      ) { %> selected <% } %>>Cancelled</option>
                                </select>

                              </td>

                        </tr>
                        <% } %>
                          <% } } else { %>
                            <tr>
                              <td colspan="8" class="text-center">
                                No such order found
                              </td>
                            </tr>
                            <% } %>
                </tbody>
              </table>
            </div>

